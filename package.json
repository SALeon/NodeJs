{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --inspect -r @babel/register ./index.js",
    "plain-text-server": "node ./http-servers/plain-text-server.js",
    "html-server": "node ./http-servers/html-server.js",
    "json-server": "node ./http-servers/json-server.js",
    "echo-server": "node ./http-servers/echo-server.js",
    "pg-init": "docker run --name postgres-0 -e POSTGRES_PASSWORD=password -e POSTGRES_DB=postgres-0 -e POSTGRES_USER=user -d -p 5430:5432 postgres:alpine",
    "pg-start": "docker start -i postgres-0",
    "pg-remove": "docker rm -f postgres-0",
    "db-migration": "node_modules/.bin/sequelize db:migrate",
    "db-migration-revert": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db-seed": "node_modules/.bin/sequelize db:seed:all",
    "sq-init": "node_modules/.bin/sequelize init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SALeon/NodeJs.git"
  },
  "author": "Siarhei Leanavets <Siarhei_Leanavets@epam.com>",
